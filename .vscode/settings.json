{
    "editor.tabSize"        : 2,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys"       : true,
    "vim.hlsearch"          : true,
    "vim.handleKeys"        : {
        "<C-f>": false,
        "<C-b>": false,
    },
    "cSpell.words": [
        "domain"
    ],
    // toggler - alt-r
    "toggler.toggles": [
        [ "and", "or" ],
        [ "exclusive", "inclusive" ],

        [ "absolute", "relative" ],
        [ "C:", "D:" ],
        [ "HDD", "SDD" ],
        [ "Install", "Uninstall" ],

        // file,algorithm, structure
        [ "stack", "queue"],
        [ "read", "write" ],
        [ "ifstream", "ofstream" ],
        [ "int", "float", "double" ],
        [ "string", "char" ],
        [ "vector", "list" ],
        [ "read", "write" ],
        [ "tree", "graph" ],
        [ "binary", "decimal" ],
        [ "bin", "usr", "tmp"],
        [ "ctrl", "alt" ],
        [ "back", "front" ],
        [ "begin", "end" ],
        [ "cbegin", "cend" ],
        [ "push_back", "pop_back" ],
        [ "insert", "erase" ],
        [ "emplace", "emplace_back" ],
        [ "edge", "vertex" ],
        [ "directed", "undirected" ],
        [ "combination", "punctuation" ],
        [ "decode", "incode" ],

        [ "interface", "extend" ],

        [ "email", "address" ],
        [ "id", "password" ],

        [ "master", "slave" ],   

        [ "ip", "mac" ],

        [ "jpg","png","gif" ],
        [ "png","ppt" ],

        [ "ctrl", "alt", "shift" ],
        [ "upArrow", "downArrow" ],   
        [ "leftArrow", "rightArrow" ],
        [ "pageUp", "pageDown" ],

        [ "upperCase", "lowercase" ],
        [ "backspace", "delete" ],

        [ "success", "fail" ],

        // math
        [ "bound", "unbound" ],
        [ "January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December"],

        [ "linux", "window"],
        [ "soundcloud", "youtube" ],
        [ "musixmatch", "genius" ],
        [ "facebook", "google" ],
        [ "naver", "daum" ],

        // OS
        [ "Created", "terminated" ],
        [ "Wating", "Running", "Blocked" ],
        [ "single", "multi"],
        [ "thread", "process"]
        [ "Hold", "wait"]

    ]
}
